        "sugar": "12g"
      },
      "ingredients": [
        {
          "amount": "2",
          "unit": "large",
          "ingredient": "ripe bananas",
          "notes": "mashed"
        },
        {
          "amount": "1/4",
          "unit": "cup",
          "ingredient": "almond butter",
          "notes": "or peanut butter"
        },
        {
          "amount": "2",
          "unit": "tbsp",
          "ingredient": "cocoa powder",
          "notes": "unsweetened"
        },
        {
          "amount": "2",
          "unit": "tbsp",
          "ingredient": "maple syrup",
          "notes": "pure"
        },
        {
          "amount": "1/4",
          "unit": "cup",
          "ingredient": "mini dark chocolate chips",
          "notes": "dairy-free if vegan"
        }
      ],
      "instructions": [
        {
          "step": 1,
          "text": "In a medium bowl, mash the ripe bananas until smooth.",
          "image": "/images/recipes/banana-bites-step1.jpg"
        },
        {
          "step": 2,
          "text": "Add almond butter and mix until well combined.",
          "image": "/images/recipes/banana-bites-step2.jpg"
        },
        {
          "step": 3,
          "text": "Stir in cocoa powder and maple syrup until the mixture is smooth and chocolatey.",
          "image": "/images/recipes/banana-bites-step3.jpg"
        },
        {
          "step": 4,
          "text": "Fold in the mini chocolate chips gently.",
          "image": "/images/recipes/banana-bites-step4.jpg"
        },
        {
          "step": 5,
          "text": "Using a small cookie scoop or spoon, form mixture into 12 bite-sized balls and place on a parchment-lined tray.",
          "image": "/images/recipes/banana-bites-step5.jpg"
        },
        {
          "step": 6,
          "text": "Refrigerate for 10-15 minutes to firm up. Serve chilled and enjoy!",
          "image": "/images/recipes/banana-bites-step6.jpg"
        }
      ],
      "notes": [
        "Store in the refrigerator for up to 5 days in an airtight container.",
        "For a nut-free version, substitute sunflower seed butter for almond butter.",
        "You can roll the bites in shredded coconut or chopped nuts for extra texture.",
        "These freeze well for up to 3 months - just thaw for 5 minutes before serving."
      ],
      "equipment": [
        "Medium mixing bowl",
        "Fork or potato masher",
        "Small cookie scoop or spoon",
        "Parchment paper",
        "Baking tray"
      ],
